#include<stdio.h>
#include<math.h>
int main(void)
{
    int x = 5;
    
    printf("Exercise 2A Table\n\n");
    printf(" x      x^2     x^3\n");
    printf("---     ---     ---\n");

    for ( x = 0; x <= 10; ++x )
    
    printf("%4i     %i       %4i\n", x, x*x,x*x*x);
    return 0;
}
